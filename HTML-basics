    <script>
      ////////////////////
      // Ajax cart code //
      ////////////////////

      function openAjaxCart() {
        $('.ajaxCart-fullContainer').removeClass('hidden').addClass('ajaxCart--active')
        var timeout = setTimeout(function() {
          closeAjaxCart()
        }, 5000)

        $('.ajaxCart').on('mouseenter', function() {
          clearTimeout(timeout)
        })
        $('.ajaxCart').on('mouseleave', function() {
          closeAjaxCart()
        })
      }

      function closeAjaxCart() {
        $('.ajaxCart-fullContainer').removeClass('ajaxCart--active').addClass('hidden')
      }

      function addItem(formId) {
        $.ajax({
          type: 'POST',
          url: '/cart/add.js',
          dataType: 'json',
          data: $('#' + formId).serialize(),
          success: addToCartOk,
          error: addToCartFail
        })
      }

      var totalQuantity = 0
      function addToCartOk(product) {
        var formQuantity = parseInt($('#AddToCartForm #Quantity').val()) || 1
        totalQuantity += parseInt(formQuantity)
        var title = product.product_title ? product.product_title : ''
        var variantTitle = product.variant_title ? product.variant_title : ''
        var type = product.product_type ? product.product_type : ''
        var price = product.price ? ((product.price / 100) * formQuantity) : 0

        if (!$('.ajaxCart-parent').length) {
          $('.ajaxCart').append('<div class="ajaxCart-parent"></div>')
          $('.ajaxCart').append('<a target="_self" href="/cart" class="btn btn--large btn--full uppercase ajaxCart-btn--checkout">Checkout</a>')
          $('.ajaxCart-parent').append('<div class="ajaxCart-container"></div>')
          $('.ajaxCart-container').append('<div class="clearfix"><div class="left ajaxCart-firstChild"></div><div class="left ajaxCart-secondChild clearfix"></div></div>')
          $('.ajaxCart-container .ajaxCart-firstChild').append('<img class="ajaxCart-productImg" src="' + product.image + '" alt=' + product.product_title + ' />')
          $('.ajaxCart-container .ajaxCart-secondChild').append('<h5>' + title + '</h5>')
          $('.ajaxCart-container .ajaxCart-secondChild').append('<h6>' + type + '</h6>')
          $('.ajaxCart-container .ajaxCart-secondChild').append('<h6 class="ajaxCart-secondChild--size">Size: ' + variantTitle + '</h6>')
          $('.ajaxCart-container .ajaxCart-secondChild').append('<h6 class="ajaxCart-secondChild--quantity">Quantity: ' + formQuantity + '</h6>')
          $('.ajaxCart-container .ajaxCart-secondChild').append('<h6 class="ajaxCart-secondChild--price">$' + price + '</h6>')
        } else {
          var newPrice = (product.price / 100) * totalQuantity
          $('.ajaxCart-secondChild--quantity').html('Quantity: ' + totalQuantity)
          $('.ajaxCart-secondChild--price').html('$' + newPrice)
        }

        getCart()
        openAjaxCart()
      }

      function addToCartFail(obj, status) {
        var error = obj.responseJSON && obj.responseJSON.description ? obj.responseJSON.description : 'The item(s) you are trying to add is out of stock.'
        $('.ajaxCart').html('<h4 class="ajaxCart-heading">There was an error adding items to your cart</h4><p class="ajaxCart-error--message">' + error + '</p><a target="_self" href="/cart" class="btn btn--large btn--full uppercase">Go to Cart</a>')

        openAjaxCart()
      }

      function getCart() {
        $.ajax({
          type: 'GET',
          url: '/cart.js',
          dataType: 'json',
          success: function(cart) {
            if (cart) {
              if (cart.item_count && cart.item_count > 0) {
                $('.cartCount-updater').html(cart.item_count)
                $('#nav-cartCountDesktop').html('(' + cart.item_count + ')')
                if ($('#nav-cartCountDesktop').hasClass("hidden-count")) {
                  $('#nav-cartCountDesktop').removeClass('hidden-count')
                }
              }
            }
          }
        })
      }
    </script>



